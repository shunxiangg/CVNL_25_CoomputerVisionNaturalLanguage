ta_test = torchvision.datasets.MNIST(\"./data\", train=False,\n","                                       download=True,\n","                                       transform=transforms.ToTensor())\n","\n","# Check if CUDA is available\n","if torch.cuda.is_available():\n","    device = torch.device(\"cuda\")\n","else:\n","    device = torch.device(\"cpu\")\n","\n","print(f\"Using device: {device}\")\n","\n","# Create DataLoaders\n","B = 32\n","mnist_train_loader = DataLoader(mnist_data_train, batch_size=B, shuffle=True)\n","mnist_test_loader = DataLoader(mnist_data_test, batch_size=B)\n","\n","# Define accuracy score function\n","def accuracy_score(y_true, y_pred):\n","    \"\"\"Calculate accuracy\"\"\"\n","    return (y_true == y_pred).float().mean().item()\n","\n","# Define training function\n","def train_simple_network(model, loss_func, train_loader, test_loader=None,\n","                        score_funcs=None, device='cpu', epochs=10):\n","    \"\"\"\n","    Train a neural network and track metrics\n","\n","    Args:\n","        model: PyTorch model\n","        loss_func: Loss function\n","        train_loader: Training data loader\n","        test_loader: Test data loader (optional)\n","        score_funcs: Dictionary of scoring functions\n","        device: Device to train on\n","        epochs: Number of training epochs\n","\n","    Returns:\n","        DataFrame with training history\n","    \"\"\"\n","    # Move model to device\n","    model = model.to(device)\n","\n","    # Optimizer\n","    optimizer = torch.optim.Adam(model.parameters(), lr=0.001)\n","\n","    # Track results\n","    results = []\n","\n","    for epoch in range(epochs):\n","        # Training phase\n","        model.train()\n","        train_loss = 0.0\n","        train_correct = 0\n","        train_total = 0\n","\n","        for batch_x, batch_y in train_loader:\n","            batch_x, batch_y = batch_x.to(device), batch_y.to(device)\n","\n","            # Forward pass\n","            optimizer.zero_grad()\n","            outputs = model(batch_x)\n","            loss = loss_func(outputs, batch_y)\n","\n","            # Backward pass\n","            loss.backward()\n","            optimizer.step()\n","\n","            # Track metrics\n","            train_loss += loss.item()\n","            _, predicted = torch.max(outputs.data, 1)\n","            train_total += batch_y.size(0)\n","            train_correct += (predicted == batch_y).sum().item()\n","\n","        # Calculate training metrics\n","        avg_train_loss = train_loss / len(train_loader)\n","        train_accuracy = train_correct / train_total\n","\n","        # Testing phase\n","        test_accuracy = None\n","        if test_loader is not None:\n","            model.eval()\n","            test_correct = 0\n","            test_total = 0\n","\n","            with torch.no_grad():\n","                for batch_x, batch_y in test_loader:\n","                    batch_x, batch_y = batch_x.to(device), batch_y.to(device)\n","                    outputs = model(batch_x)\n","                    _, predicted = torch.max(outputs.data, 1)\n","                    test_total += batch_y.size(0)\n","                    test_correct += (predicted == batch_y).sum().item()\n","\n","            test_accuracy = test_correct / test_total\n","\n","        # Store results\n","        result = {\n","            'epoch': epoch + 1,\n","            'train_loss': avg_train_loss,\n","            'train Accuracy': train_accuracy,\n","        }\n","        if test_accuracy is not None:\n","            result['test Accuracy'] = test_accuracy\n","\n","        results.append(result)\n","\n","        # Print progress\n","        print(f\"Epoch {epoch+1}/{epochs} - \"\n","              f\"Loss: {avg_train_loss:.4f}, \"\n","              f\"Train Acc: {train_accuracy:.4f}\", end=\"\")\n","        if test_accuracy is not None:\n","            print(f\", Test Acc: {test_accuracy:.4f}\")\n","        else:\n","            print()\n","\n","    return pd.DataFrame(results)\n","\n","# Define model parameters\n","D = 28*28  # 28 * 28 images\n","C = 1      # No of channels in input (grayscale)\n","classes = 10  # Digits 0-9\n","filters = 16  # Number of convolutional filters\n","K = 3      # Size of Kernel\n","\n","# Linear model (fully connected) for comparison\n","model_linear = nn.Sequential(\n","    nn.Flatten(),  # (B, C, W, H) -> (B, C*W*H) = (B, D)\n","    nn.Linear(D, 256),\n","    nn.Tanh(),\n","    nn.Linear(256, classes),\n",")\n","\n","# Simple convolutional network\n","model_cnn = nn.Sequential(\n","    nn.Conv2d(C, filters, K, padding=K//2),  # x convolve with G\n","    nn.Tanh(),\n","    nn.Flatten(),\n","    nn.Linear(filters*D, classes),\n",")\n","\n","# Define loss function\n","loss_func = nn.CrossEntropyLoss()\n","\n","print(\"\\n\" + \"=\"*60)\n","print(\"Training CNN Model\")\n","print(\"=\"*60)\n","cnn_results = train_simple_network(model_cnn, loss_func,\n","                      mnist_train_loader, test_loader=mnist_test_loader,\n","                      score_funcs={'Accuracy': accuracy_score},\n","                      device=device, epochs=10)\n","\n","print(\"\\n\" + \"=\"*60)\n","print(\"Training Fully Connected Model\")\n","print(\"=\"*60)\n","fc_results = train_simple_network(model_linear, loss_func,\n","                       mnist_train_loader, test_loader=mnist_test_loader,\n","                       score_funcs={'Accuracy': accuracy_score},\n","                       device=device, epochs=10)\n","\n","# Plot comparison\n","plt.figure(figsize=(10, 6))\n","plt.plot(cnn_results['epoch'], cnn_results['test Accuracy'],\n","         marker='o', label='CNN', linewidth=2)\n","plt.plot(fc_results['epoch'], fc_results['test Accuracy'],\n","         marker='s', label='Fully Connected', linewidth=2)\n","plt.xlabel('Epoch', fontsize=12)\n","plt.ylabel('Test Accuracy', fontsize=12)\n","plt.title('CNN vs Fully Connected Network Performance', fontsize=14, fontweight='bold')\n","plt.legend(fontsize=11)\n","plt.grid(True, alpha=0.3)\n","plt.ylim([0.8, 1.0])\n","plt.tight_layout()\n","plt.savefig('cnn_vs_fc_accuracy.png', dpi=150, bbox_inches='tight')\n","plt.show()\n","\n","# Print final results\n","print(\"\\n\" + \"=\"*60)\n","print(\"FINAL RESULTS\")\n","print(\"=\"*60)\n","print(f\"CNN Final Test Accuracy: {cnn_results['test Accuracy'].iloc[-1]:.4f}\")\n","print(f\"FC Final Test Accuracy: {fc_results['test Accuracy'].iloc[-1]:.4f}\")\n","print(f\"\\nCNN is better by: {(cnn_results['test Accuracy'].iloc[-1] - fc_results['test Accuracy'].iloc[-1])*100:.2f}%\")"],"metadata":{"id":"oDXW28OPSJrT","colab":{"base_uri":"https://localhost:8080/","height":1000},"executionInfo":{"status":"ok","timestamp":1764401061987,"user_tz":-480,"elapsed":247770,"user":{"displayName":"Shun Xiang Ang","userId":"08501829227407716874"}},"outputId":"6605123c-befe-406e-87d2-cbbdddf7491f"},"execution_count":1,"outputs":[{"output_type":"stream","name":"stderr","text":["100%|██████████| 9.91M/9.91M [00:00<00:00, 14.4MB/s]\n","100%|██████████| 28.9k/28.9k [00:00<00:00, 340kB/s]\n","100%|██████████| 1.65M/1.65M [00:00<00:00, 3.17MB/s]\n","100%|██████████| 4.54k/4.54k [00:00<00:00, 12.4MB/s]\n"]},{"output_type":"stream","name":"stdout","text":["Using device: cuda\n","\n","============================================================\n","Training CNN Model\n","============================================================\n","Epoch 1/10 - Loss: 0.3558, Train Acc: 0.8965, Test Acc: 0.9206\n","Epoch 2/10 - Loss: 0.2611, Train Acc: 0.9254, Test Acc: 0.9328\n","Epoch 3/10 - Loss: 0.2107, Train Acc: 0.9396, Test Acc: 0.9443\n","Epoch 4/10 - Loss: 0.1627, Train Acc: 0.9530, Test Acc: 0.9520\n","Epoch 5/10 - Loss: 0.1285, Train Acc: 0.9612, Test Acc: 0.9566\n","Epoch 6/10 - Loss: 0.1100, Train Acc: 0.9671, Test Acc: 0.9642\n","Epoch 7/10 - Loss: 0.0960, Train Acc: 0.9712, Test Acc: 0.9635\n","Epoch 8/10 - Loss: 0.0849, Train Acc: 0.9741, Test Acc: 0.9619\n","Epoch 9/10 - Los